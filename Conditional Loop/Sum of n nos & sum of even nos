# 1.Sum of n nos
n=int(input())
a=1
b=0
while a<=n:
    b=b+a
    a=a+1
print(b)


  
# 2. sum of even nos
n=int(input())
a=1
b=0
while a<=n:
    if a%2==0:
        b=b+a
    a=a+1
print(b) 


# 3. Farenhit to celcius

S = int(input())
E = int(input())
W = int(input())
while S <= E:
    a = int((S-32)*0.5556)
   
    print(S, a)
    S = S + W
