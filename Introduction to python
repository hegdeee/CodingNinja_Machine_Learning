# 1. AVEREGE MARKS
a=int(input())
b=int(input())
c=int(input())
avg=(a+b+c)/3
print(avg)



# 2. Find X^n 
def myPow(x: float, n: int) -> float:
    sqt=pow(x,n)
    return sqt

# 3.Arithmetic Progression
a=int(input())
